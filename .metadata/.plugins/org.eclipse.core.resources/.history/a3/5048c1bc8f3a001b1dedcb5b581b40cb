package ch02;

public class ImplicitConversion {
	public void implicitConversion() {
		
		System.out.println("----- 자동 타입 변환 -----");
		byte bNum = 10;
		int iNum = bNum;
		
		System.out.println(bNum);
		System.out.println(iNum);
		
		int iNum2 = 20;
		float fNum = iNum2;
		
		System.out.println(iNum);
		System.out.println(fNum);
		
		double dNum;
		dNum = fNum + iNum;
		System.out.println(dNum);
		
	}
	
}
