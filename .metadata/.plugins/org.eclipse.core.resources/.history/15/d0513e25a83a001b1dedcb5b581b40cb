package ch03;

public class Q1 {
	public void q1() {
		int myAge = 23;
		int teacherAge = 38;
		
		boolean value = (myAge > 25);
		System.out.println(value);
		
		System.out.println(myAge <= 25);
		System.out.println(myAge == teacherAge);
		
		char ch;
		ch = (myAge>teacherAge) ? 'T' : 'F';
		
		System.out.println(ch);
	}
}
